{
  "name": "solitaire",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "clean": "rimraf build",
    "build:webpack": "webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "start": "set NODE_PATH='source' && node devServer.js",
    "pretest": "cls",
    "test": "ava | faucet",
    "watch": "npm test & onchange source/**/*.js -- npm test",
    "update": "updtr"
  },
  "author": "James Brown",
  "license": "CC0-1.0",
  "devDependencies": {
    "ava": "^0.10.0",
    "babel-cli": "6.4.0",
    "babel-core": "6.4.0",
    "babel-eslint": "4.1.6",
    "babel-loader": "6.2.1",
    "babel-plugin-react-transform": "2.0.0",
    "babel-plugin-transform-runtime": "6.4.0",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-react": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "babel-register": "^6.4.3",
    "cheerio": "0.19.0",
    "deep-freeze": "0.0.1",
    "enzyme": "1.2.0",
    "eslint": "1.10.3",
    "eslint-plugin-react": "3.14.0",
    "express": "4.13.3",
    "faucet": "0.0.1",
    "jsdom": "7.2.2",
    "onchange": "^2.0.0",
    "react-addons-test-utils": "0.14.6",
    "react-dom": "0.14.6",
    "redbox-react": "1.2.0",
    "redux": "3.0.5",
    "rimraf": "2.5.0",
    "updtr": "0.1.6",
    "webpack": "1.12.10",
    "webpack-dev-middleware": "1.4.0",
    "webpack-hot-middleware": "2.6.0"
  },
  "dependencies": {
    "immutable": "^3.7.6",
    "ramda": "^0.19.1",
    "react": "0.14.6",
    "react-bootstrap": "^0.28.2",
    "react-dom": "0.14.3",
    "react-redux": "^4.0.6"
  },
  "ava": {
    "files": [
      "source/test/**/*.js",
      "!source/test/testGroup.js"
    ],
    "failFast": true,
    "serial": true,
    "tap": true,
    "verbose": false,
    "require": [
      "babel-core/register"
    ]
  }
}
